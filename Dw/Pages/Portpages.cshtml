@page "{Id}"
@model Dogwood.Pages.PortpagesModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Portpages</title>
</head>
<body>
    <div class="grid are-images-unloaded">
        <div class="grid-sizer col-12 col-sm-6 col-md-4"></div>

        @foreach (var item in Model.imageLinks)
    {
        <div class="grid-item col-12 col-sm-6 col-md-4">
            <div class="grid-item-content pb-4">
                @Html.Raw(item)
            </div>
        </div>
}

    </div>
</body>
</html>
